@{
    ViewData["Title"] = "Home Page";
}

<div class="container-fluid" id="home-page-bg">
    <div id="container" class="container-fluid">

        <div class="row">

            <div class="col-md-6">

                <div class="white-content-holder">
                    <h4>Priser</h4>
                    <ul>
                        <li><strong>Privat Kunder: </strong>600 kr per timme</li>
                    </ul>
                    <p>
                        Exempel: Ni parkerar 2:08 och betalar då 1280 kr.
                    </p>
                </div>


            </div>

            <div class="col-md-6">

                <div class="white-content-holder">
                    <h4>Garaget V2.5</h4>
                    <p>Är byggt i ASP .NET Core , CSS3 och Bootstrap !</p>
